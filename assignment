import math
length = input("input a length for a rectangle: ")
width = input("input a width for a rectangle: ")
length = int(length)
width = int(width)
area1 = length * width
print ("the area of the rectangle is: ", area1)

radius = input("input a radius for the circle: ")
radius = int(radius)

area_of_circle = math.pi * math.pow(radius,2)
print("the area of the circle is: ", area_of_circle)
