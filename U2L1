x = input("enter a grade percentage here")
x = int(x)
if (x >= 80) and (x <= 100):
  print ("your grade is an A")
if (x >= 70) and (x <= 79):
  print ("your grade is an B")
if (x >= 60) and (x <= 69):
  print ("your grade is an C")
if (x >= 50) and (x <= 59):
  print ("your grade is an A")
if (x < 50):
  print ("your grade is an F")
